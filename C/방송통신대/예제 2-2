#include <stdio.h>
#define AGE 28
#define PI 3.141592
#define NAME "홍길동"

const int a=10;
const float b=10.56789;
const char c='K';
const char* KNOU="한국방송통신대학교";

void main(){
	int i = 28;
	float j = 4.0;
	char k = 'A';
	char ch[10] = "홍길동";
	char *p = "컴퓨터과학과";
	
	printf("학교는: %s 이름은: %s 나이는: %d\n", KNOU, NAME, AGE);
	printf("%s %s 나이:%d 학점:%c 평점:%f\n", p, ch, i, k, j);
}
