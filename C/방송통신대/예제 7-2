#include <stdio.h>

struct member
{
	char *name;
	char *sex;
	int age;
};

void main()
{
	int i;
	struct member
	{
		char *name;
		char *sex;
		int age;
	};
	
	struct member a[4] = {{"Hong, kil-dong", "mail", 50},{"Choi, Soon-Hee", "female", 46},{"Hong, Heon-Jeong", "female", 3},{"Hing, Yoo-Shin", "male", 2}};
						 
	for(i=0; i<4; i++){
		printf("%s %s %d \n", a[i].name, a[i].sex, a[i].age);
	}
}
