#include <stdio.h>

void main(){
	long i, sum1 = 0;
	float sum2 = 0;
	double sum3 = 0;
	
	for (i=10000; i<=11000; i++){
		sum1 = sum1 + i;
		sum2 = sum2 + i;
		sum3 = sum3 + i;
	}
	printf("10,000~11,000의 합(long) : %d\n", sum1);
	printf("10,000~11,000의 합(float) : %d\n", sum2);
	printf("10,000~11,000의 합(double) : %d\n", sum3);
}
